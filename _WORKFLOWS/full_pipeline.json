{\n  "name": "Full Pipeline Workflow",\n  "on": {\n    "push": {\n      "branches": ["main"]\n    }\n  },\n  "jobs": {\n    "build": {\n      "runs-on": "ubuntu-latest",\n      "steps": [\n        {\n          "name": "Checkout Code",\n          "uses": "actions/checkout@v2"\n        },\n        {\n          "name": "Set up Node.js",\n          "uses": "actions/setup-node@v2",\n          "with": {\n            "node-version": "14"\n          }\n        },\n        {\n          "name": "Install Dependencies",\n          "run": "npm install"\n        },\n        {\n          "name": "Run Tests",\n          "run": "npm test"\n        },\n        {\n          "name": "Build Project",\n          "run": "npm run build"\n        }\n      ]\n    }\n  }\n}